{
	"manifest_version": 2,
	"name": "active_navigation",
	"version": "0.1.0",
	"description": "Active Navigation allows easy navigation among open tabs.",
	"browser_action": {
		"default_title": "Active Navigation"
	},
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"icons": {
		"128": "images/compass-icon-128.png",
		"48": "images/compass-icon-48.png",
		"16": "images/compass-icon-16.png"
	},
	"permissions": ["tabs", "contextMenus"]
}